   "source": [
    "import turtle\n",
    "\n",
    "colors = ['indigo', 'purple', 'medium purple', 'blue violet', 'dark violet', 'dark orchid']\n",
    "t = turtle.Pen()\n",
    "t.clear()\n",
    "\n",
    "turtle.bgcolor('black')\n",
    "for x in range(200):\n",
    "    t.pencolor(colors[x%6])\n",
    "    t.width(x//100+1)\n",
    "    t.forward(x)\n",
    "    t.left(50)\n",
    "    t.backward(x/2)"
   ]
  }
